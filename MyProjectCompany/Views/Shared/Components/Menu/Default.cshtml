@model IEnumerable<ServiceDTO> 

    <nav id="nav">
        <ul>
            <li>
                <a asp-area="" asp-controller="Home" asp-action="Index">Головна</a>
            </li>
            <li>
                <a asp-area="" asp-controller="Services" asp-action="Index">Послуги</a>
                <ul>
                    @foreach(ServiceDTO entity in Model)
                {
                    <li>
                        <a asp-area="" asp-controller="Services" asp-action="Show" asp-route-id="@entity.Id">
                            @entity.Title
                        </a>
                    </li>
                }
            </ul>
            </li>
            <li>
            <a asp-area="" asp-controller="Home" asp-action="Contacts">Контакти</a>
            </li>
        </ul>
    </nav>


